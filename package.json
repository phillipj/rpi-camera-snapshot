{
  "name": "rpi-camera-stream",
  "version": "1.0.0",
  "description": "Taking camera photos with a Raspberry Pi",
  "main": "backend/server.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "npm run build:make && npm run build:minify",
    "build:make": "rimraf public/build && elm make frontend/Main.elm --output public/build/compiled.js --warn --yes",
    "build:minify": "uglifyjs --output=public/build/compiled.min.js --mangle --compress warnings=false -- public/build/compiled.js",
    "start": "elm-live frontend/Main.elm --dir=public --output=public/build/compiled.js --open"
  },
  "private": true,
  "keywords": [
    "raspberry",
    "raspberry-pi",
    "camera"
  ],
  "author": "Phillip Johnsen <johphi@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "compression": "1.6.2",
    "elm": "^0.18.0",
    "express": "^4.13.4",
    "raspicam": "0.2.13"
  },
  "devDependencies": {
    "elm-live": "2.7.4",
    "rimraf": "2.5.4",
    "uglify-js": "2.7.5"
  }
}
